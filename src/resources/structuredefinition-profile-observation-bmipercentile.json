{
  "resourceType":"StructureDefinition",
  "id":"profile-observation-bmipercentile",
  "url":"http://hl7.org/fhir/ca/core/StructureDefinition/profile-observation-bmipercentile",
  "version":"0.1.0",
  "name":"Observation (Percentile BMI)",
  "title":"Observation Profile (Percentile BMI)",
  "status":"draft",
  "date":"2020-06-16",
  "publisher":"Canada Health Infoway",
  "description":"Proposed constraints on the Observation resource for use in storing and querying Body mass index (BMI) [Percentile]",
  "jurisdiction": [
    {
      "coding": [
        {
          "system":"urn:iso:std:iso:3166",
          "code":"CA",
          "display":"Canada"
        }
      ]
    }
  ],
  "kind":"resource",
  "abstract":false,
  "type":"Observation",
  "baseDefinition":"http://hl7.org/fhir/StructureDefinition/Observation",
  "derivation":"constraint",
  "differential":{
    "element": [
      {
        "id":"Observation",
        "path":"Observation",
        "short":"Observation Profile",
        "definition":"The Observation Ratio BMI Profile is based upon the core FHIR Observation Resource"
      },
      {
        "id":"Observation.identifier",
        "path":"Observation.identifier",
        "mustSupport":true
      },
      {
        "id":"Observation.category",
        "path":"Observation.category",
        "slicing":{
          "discriminator": [
            {
              "type":"pattern",
              "path":"type"
            }
          ],
          "rules":"open"
        },
        "min":1,
        "mustSupport":true
      },
      {
        "id":"Observation.category:vital-signs",
        "path":"Observation.category",
        "sliceName":"vital-signs",
        "short":"FHIR standard classification of type of observation",
        "min":1,
        "patternCodeableConcept":{
          "coding": [
            {
              "system":"http://terminology.hl7.org/CodeSystem/observation-category",
              "code":"vital-signs"
            }
          ]
        },
        "mustSupport":true
      },
      {
        "id":"Observation.code",
        "path":"Observation.code",
        "short":"Percentile BMI: LOINC 59576-9",
        "definition":"Type of observation: Limited to fixed LOINC value 59576-9 = Body mass index (BMI) [Percentile]",
        "min":1,
        "max":"1",
        "type": [
          {
            "code":"CodeableConcept"
          }
        ],
        "patternCodeableConcept":{
          "coding": [
            {
              "system":"http://loinc.org",
              "code":"39156-5"
            }
          ]
        },
        "mustSupport":true
      },
      {
        "id":"Observation.subject",
        "path":"Observation.subject",
        "min":1,
        "type": [
          {
            "code":"Reference",
            "targetProfile": [
              "http://hl7.org/fhir/ca/core/StructureDefinition/profile-patient"
            ]
          }
        ],
        "mustSupport":true
      },
      {
        "id":"Observation.effective[x]",
        "path":"Observation.effective[x]",
        "type": [
          {
            "code":"dateTime"
          },
          {
            "code":"Period"
          }
        ],
        "constraint": [
          {
            "key":"ca-core-datetime",
            "severity":"error",
            "human":"Datetime SHALL be at least to day",
            "expression":"($this as dateTime).toString().length() >= 8"
          }
        ],
        "mustSupport":true
      },
      {
        "id":"Observation.value[x]",
        "path":"Observation.value[x]",
        "slicing":{
          "discriminator": [
            {
              "type":"type",
              "path":"$this"
            }
          ],
          "rules":"open"
        },
        "constraint": [
          {
            "key":"ca-core-results-1",
            "severity":"error",
            "human":"Observation.value[x] or Observation.dataAbsentReason SHALL be present",
            "expression":"value[x].exists() or dataAbsentReason.exists()",
            "xpath":"Observation.value[x] or Observation.dataAbsentReason"
          }
        ],
        "mustSupport":true
      },
      {
        "id":"Observation.value[x]:valueQuantity",
        "path":"Observation.value[x]",
        "sliceName":"valueQuantity",
        "type": [
          {
            "code":"Quantity"
          }
        ]
      },
      {
        "id":"Observation.value[x]:valueQuantity.value",
        "path":"Observation.value[x].value",
        "min":1
      },
      {
        "id":"Observation.value[x]:valueQuantity.unit",
        "path":"Observation.value[x].unit",
        "min":1
      },
      {
        "id":"Observation.value[x]:valueQuantity.system",
        "path":"Observation.value[x].system",
        "min":1,
        "fixedUri":"http://unitsofmeasure.org",
        "constraint": [
          {
            "key":"ca-core-results-2",
            "severity":"error",
            "human":"SHALL use UCUM for coded quantity units",
            "expression":"where(valueQuantity.system='http://unitsofmeasure.org' and valueQuantity.value.exists()).exists()",
            "xpath":"exists(f:valueQuantity/value) and exists(f:valueQuantity/system[@value='http://unitsofmeasure.org'])"
          }
        ]
      },
      {
        "id":"Observation.dataAbsentReason",
        "path":"Observation.dataAbsentReason",
        "constraint": [
          {
            "key":"ca-core-results-1",
            "severity":"error",
            "human":"Observation.value[x] or Observation.dataAbsentReason SHALL be present",
            "expression":"value[x].exists() or dataAbsentReason.exists()",
            "xpath":"Observation.value[x] or Observation.dataAbsentReason"
          }
        ],
        "mustSupport":true
      },
      {
        "id":"Observation.interpretation",
        "path":"Observation.interpretation",
        "mustSupport":true
      },
      {
        "id":"Observation.referenceRange",
        "path":"Observation.referenceRange",
        "mustSupport":true
      }
    ]
  }
}
